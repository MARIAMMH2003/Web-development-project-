    <section id="home" class="parallax">
        <div class="parallax-container">
            <div class="header">
                <h1>Tourify</h1>
                <h3>Explore Historical Wonders</h3>
                <button class="rounded-button" onclick="scrollToSection('museums')">Get Started</button>
            </div>
            <img src="../images/sun.png" class="sun" alt="Sun">
            <img src="../images/cloud1.png" class="cloud1" alt="Cloud 1">
            <img src="../images/cloud2.png" class="cloud2" alt="Cloud 2">
