    <section id="museums">
        <h1 class="text-header">Museums</h1>
        <div class="container-museum">
            <div class="slide">
                <% museums.forEach(museum => { %>
                    <div class="item" style="background-image: url('<%= museum.picture %>');">
                        <div class="content">
                            <div class="name"><%= museum.title %></div>
                            <h3 class="museum-rating">
                                <span><%= museum.rating %></span>
                                <div class="rating">
                                    <% for(let i = 0; i < Math.floor(museum.rating); i++) { %>
                                        <ion-icon name="star"></ion-icon>
                                    <% } %>
                                    <% if (museum.rating % 1 !== 0) { %>
                                        <ion-icon name="star-half"></ion-icon>
                                    <% } %>
                                </div>
                            </h3>
                            <a href="/<%= museum._id %>" class="button-link">See More</a>
                        </div>
                        <div class="price">
                            <%= museum.price %>$
                          </div>
                    </div>
                <% }) %>
            </div>
            <div class="button">
                <button class="prev"><i class="fa-solid fa-arrow-left"></i></button>
                <button class="next"><i class="fa-solid fa-arrow-right"></i></button>
            </div>
        </div>
    </section>
    
    <section class="wide-card-section">
        <div class="wide-card">
